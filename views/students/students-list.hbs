<div class="container">


    <h1>Students list</h1>
    <hr>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Student</th>
            </tr>
        </thead>
        <tbody>

            {{#each createdUser}}

            <tr>

                <td>Nombre:{{username}}</td>

                <td>
                    <a href="/students/{{_id}}" class="btn btn-dark btn-sm">Ver</a>
                </td>

            </tr>

            {{/each}}

        </tbody>
    </table>
</div>
</div>

<hr>

<a href="/" class="btn btn-dark">Go back to main page.</a>